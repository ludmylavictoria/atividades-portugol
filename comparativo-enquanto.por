programa {
  funcao inicio() {
     //Comparando o uso de enquanto e faça-enquanto
   
    logico condicao
    

    condicao = falso 

    enquanto (condicao){
    escreva ("Mensagem dentro de enquanto\n")
    }
    escreva ("Fora do enquanto\n")

    condicao = falso 
  
    faca{
    escreva ("Mensagem dentro de Faça-enquanto\n")
    } enquanto(condicao)
    escreva("Fora do Faça-Enquanto\n")

  }
}
